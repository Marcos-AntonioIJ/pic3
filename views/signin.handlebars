
<header>

        <a class="Title-Header" href="index.html">Logo</a>
</header>
<main>
    <form action="/login" method="POST">
        <fieldset class="login-field">
            <p class="title-login-field">Fazer login<p>
            <p class="subtitle-login-field">Use sua conta empresarial</p>
            <input class="input-bars input-bars-cnpj" type="text" name="Cnpj" id="Cnpj" placeholder="CNPJ">
            <input class="input-bars input-bars-password" type="password" name="Password" id="Password" placeholder="Senha">
            <button class="login-field-btn submit-btn" type="submit"></button>
            <button class="login-field-btn exit-btn" type="reset"></button>
        </fieldset>
    </form>
</main>